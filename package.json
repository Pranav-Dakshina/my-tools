{
  "name": "my-tools",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://www.pranav-dakshina.com/",
  "dependencies": {
    "apollo-server-express": "^2.14.2",
    "axios": "^0.18.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.3.1",
    "classnames": "^2.2.6",
    "cors": "^2.8.5",
    "eslint-plugin-jest": "^22.3.2",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
